<p id="notice"><%= notice %></p>

<h1>Clients</h1>

<table>
  <thead>
    <tr>
      <th>Nominative</th>
      <th>Email</th>
      <th>Via</th>
      <th>Civico</th>
      <th>Cap</th>
      <th>Citta</th>
      <th>Provincia</th>
      <th>Stato</th>
      <th>Numero</th>
      <th>Partita iva</th>
      <th>Codice fiscale</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @clients.each do |client| %>
      <tr>
        <td><%= client.nominative %></td>
        <td><%= client.email %></td>
        <td><%= client.via %></td>
        <td><%= client.civico %></td>
        <td><%= client.cap %></td>
        <td><%= client.citta %></td>
        <td><%= client.provincia %></td>
        <td><%= client.stato %></td>
        <td><%= client.numero %></td>
        <td><%= client.partita_iva %></td>
        <td><%= client.codice_fiscale %></td>
        <td><%= link_to 'Show', client %></td>
        <td><%= link_to 'Edit', edit_client_path(client) %></td>
        <td><%= link_to 'Destroy', client, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Client', new_client_path %>
